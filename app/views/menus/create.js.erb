<% if @dinner.menu.election? %>
  $('#menu').html("<%= j render partial: 'menu_election' %>");
  $(".countdown").each(function(){
        $(this).countdown({
        date: $(this).children('.expiredate').attr("value"),
        format: "on"
        });
    });


<% else %>
  $('#menu').html("<%= j render partial: 'menu' %>");
<% end %>